# MinIO Credentials Secret Template
# Copy this to minio-secret.yaml and fill in values
# For local dev, use simple credentials

apiVersion: v1
kind: Secret
metadata:
  name: minio-credentials
  namespace: minio-system
type: Opaque
stringData:
  MINIO_ROOT_USER: "tesslate-admin"
  MINIO_ROOT_PASSWORD: "tesslate-secret-key-change-in-prod"
